{
  "name": "@style/http-queue",
  "version": "1.0.0",
  "description": "A fire-and-forget HTTP request queue implementation for browsers.",
  "author": "SNM Style Net Media GmbH",
  "contributors": [
    {
      "name": "André König",
      "email": "andre.koenig@stylelounge.de"
    }
  ],
  "scripts": {
    "build": "./scripts/build",
    "lint": "scripty",
    "test": "scripty",
    "typings": "scripty",
    "validate-commit-message": "scripty",
    "release": "scripty"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint && npm test",
      "commit-msg": "npm run validate-commit-message"
    },
    "validate-commit-msg": {
      "warnOnFail": true
    }
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StyleLounge/http-queue.git"
  },
  "keywords": [
    "http",
    "queue",
    "browsers"
  ],
  "license": "SEE LICENSE IN license.md",
  "bugs": {
    "url": "https://github.com/StyleLounge/http-queue/issues"
  },
  "homepage": "https://github.com/StyleLounge/http-queue#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "ghooks": "^1.3.2",
    "mocha": "^2.5.3",
    "scripty": "^1.6.0",
    "standard-version": "github:akoenig/bumper",
    "ts-node": "^0.9.3",
    "tslint": "^3.13.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "validate-commit-message": "^3.0.1"
  },
  "dependencies": {
    "basil.js": "^0.4.4",
    "debug": "^2.2.0",
    "http-client": "^4.1.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.13.1",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.1",
    "redux-saga": "^0.11.0"
  }
}